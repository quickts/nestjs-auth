{
  "name": "@quickts/nestjs-auth",
  "version": "2.0.0",
  "description": "A quickts project",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register --recursive -R spec test/*.ts"
  },
  "keywords": [
    "quickts",
    "nestjs-auth"
  ],
  "author": "quickts",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^7.6.7",
    "@nestjs/core": "^9.0.5",
    "@nestjs/platform-express": "^7.6.7",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/passport-jwt": "^3.0.3",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickts/nestjs-auth.git"
  },
  "bugs": "https://github.com/quickts/nestjs-auth/issues",
  "homepage": "https://github.com/quickts/nestjs-auth#readme"
}